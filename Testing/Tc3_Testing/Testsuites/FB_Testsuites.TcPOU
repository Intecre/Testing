<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_Testsuites" Id="{f57812b9-99c8-4f64-b54a-807a33ca5cc6}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'reflection'} 
FUNCTION_BLOCK PUBLIC FB_Testsuites EXTENDS FB_Base IMPLEMENTS I_Testsuites
VAR_INPUT

END_VAR
VAR_OUTPUT
END_VAR
VAR
	nIndex: 			DINT;
	fbFile:				FB_File;
	fbBuilder:			FB_StringBuilder;
	fbList:				FB_ListSTRING;
	fbListTestsuite:	FB_ListTestsuite;
	stTestsuites:    	ST_Testsuites;
	sFilePathName:		STRING(255):= 'C:\report.xml';
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[SUPER^();]]></ST>
    </Implementation>
    <Method Name="M_Add" Id="{341dc9d1-6013-01b3-2ca0-fe24c585f169}">
      <Declaration><![CDATA[METHOD PUBLIC M_Add : BOOL
VAR_INPUT
	iTestsuite:		I_Testsuite;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[fbListTestsuite.M_Add(iTestsuite);
iTestsuite.P_Parent:= this^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_Started" Id="{9f9f5ca1-441c-0f29-0c6d-16c04b2958a4}">
      <Declaration><![CDATA[METHOD PROTECTED M_Started : BOOL
VAR_INPUT
END_VAR

VAR
	nIndex: DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[M_Started:= TRUE;
WHILE nIndex < fbListTestsuite.P_Length DO
	M_Started:= M_Started AND (fbListTestsuite.M_Get(nIndex).P_State = E_Base.Stopped);
	nIndex:= nIndex + 1;
END_WHILE]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_Starting" Id="{a8b1fe70-9112-024f-0be9-355ea21512f0}">
      <Declaration><![CDATA[METHOD PROTECTED M_Starting : BOOL
VAR_INPUT
END_VAR

VAR
	nIndex: DINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE nState OF
	0:
		IF 	SUPER^.M_Starting() THEN
			fbList.M_Clear();
			stTestsuites.sName:= F_GetName(sInstancePath);
			stTestsuites.nTests:= 0;
			stTestsuites.nFailures:= 0;
			stTestsuites.nErrors:= 0;
			stTestsuites.nSkipped:= 0;
			stTestsuites.nAssertions:= 0;
			stTestsuites.fTime:= 0.0;
			nState:= 1;
		END_IF
	
	1:
		WHILE nIndex < fbListTestsuite.P_Length DO
			fbListTestsuite.M_Get(nIndex).M_Request(E_Base.Starting);
			nIndex:= nIndex + 1;
		END_WHILE
		nState:= 2;
		
	2:
		M_Starting:= TRUE;
		
END_CASE]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_Stopping" Id="{188bbc9a-c2f6-0b60-00aa-0b55afe47559}">
      <Declaration><![CDATA[METHOD PROTECTED M_Stopping : BOOL
VAR_INPUT
END_VAR
VAR
	stTestsuite: ST_Testsuite;
	sValue: STRING(255);
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE nState OF
	0:
		nIndex:= 0;
		WHILE nIndex < fbListTestsuite.P_Length DO
			stTestsuite:= fbListTestsuite.M_Get(nIndex).P_Result;
			stTestsuites.nTests:= stTestsuites.nTests + stTestsuite.nTests;
			stTestsuites.nFailures:= stTestsuites.nFailures + stTestsuite.nFailures;
			stTestsuites.nErrors:= stTestsuites.nErrors + stTestsuite.nErrors;
			stTestsuites.nSkipped:= stTestsuites.nSkipped + stTestsuite.nSkipped;
			stTestsuites.nAssertions:= stTestsuites.nAssertions + stTestsuite.nAssertions;
			stTestsuites.fTime:= stTestsuites.fTime +  stTestsuite.fTime;
			nIndex:= nIndex + 1;
		END_WHILE
		nState:= 1;
		
	1:
//<testsuites name="Test run" tests="8" failures="1" errors="1" skipped="1"  assertions="20" time="16.082687" timestamp="2021-04-02T15:48:23">
		fbList.M_Insert(0, '<?xml version="1.0" encoding="UTF-8"?>');
		fbBuilder.M_Reset();
		fbBuilder.M_Append('<testsuites name="');	
		fbBuilder.M_Append(stTestsuite.sName);		
		fbBuilder.M_Append('" tests="');	
		fbBuilder.M_AppendInt(stTestsuite.nTests);
		fbBuilder.M_Append('" failures="');	
		fbBuilder.M_AppendInt(stTestsuite.nFailures);
		fbBuilder.M_Append(' errors="');	
		fbBuilder.M_AppendInt(stTestsuite.nErrors);
		fbBuilder.M_Append('" skipped="');	
		fbBuilder.M_AppendInt(stTestsuite.nSkipped);
		fbBuilder.M_Append('"  assertions="');	
		fbBuilder.M_AppendInt(stTestsuite.nAssertions);
		fbBuilder.M_Append('" time="');	
		fbBuilder.M_AppendReal(stTestsuite.fTime, 4);
		fbBuilder.M_Append('" timestamp="');
		fbBuilder.M_Append(stTestsuite.sTimestamp);
		fbBuilder.M_Append('">');
		fbBuilder.M_Copy(sValue);
		fbList.M_Insert(1, sValue);
		fbList.M_Add('</testsuites>');
		nState:= 2;
		
	2:
		IF fbFile.M_Delete(sFilePathName) THEN
			nState:= 3;
		END_IF	
	
	3:
		IF fbFile.M_Create(sFilePathName) THEN
			nState:= 4;
			nIndex:= 0;
		END_IF
	
	4:
			IF fbFile.M_Append(fbList.M_Item(nIndex)) THEN
				IF nIndex > fbList.P_Length THEN
					nState:= 5;
				END_IF
				nIndex:= nIndex +1;
			END_IF	
	5:
		M_Stopping:= fbFile.M_Close();	
		
END_CASE]]></ST>
      </Implementation>
    </Method>
    <Property Name="P_List" Id="{18052295-5436-0cc4-08da-cc28d1b6063a}">
